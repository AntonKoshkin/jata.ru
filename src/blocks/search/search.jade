mixin search
	if attributes.mods
		if typeof attributes.mods === 'string'
			-var mods = 'search--' + attributes.mods
		else
			-var mods = []
			each mod, i in attributes.mods
				-mods[i] = 'search--' + mod

	.search(class=mods)
		.search__content
			.search__map
				.search__car
			if block
				block